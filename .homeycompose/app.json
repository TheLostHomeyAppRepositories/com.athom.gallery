{
  "id": "com.athom.gallery",
  "version": "3.0.0",
  "compatibility": ">=8.0.0",
  "sdk": 3,
  "name": {
    "en": "Gallery"
  },
  "description": {
    "en": "Your Image Gallery"
  },
  "brandColor": "#7620DF",
  "category": [
    "tools"
  ],
  "permissions": [],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Emile Nijssen",
    "email": "emile@athom.nl"
  },
  "api": {
    "getImages": {
      "method": "GET",
      "path": "/image"
    },
    "getImage": {
      "method": "GET",
      "path": "/image/:id"
    },
    "createImage": {
      "method": "POST",
      "path": "/image"
    },
    "deleteImage": {
      "method": "DELETE",
      "path": "/image/:id"
    }
  }
}